
%install
rm -rf $RPM_BUILD_ROOT
mkdir  $RPM_BUILD_ROOT
cd     $RPM_BUILD_ROOT

mkdir -p  ./usr/java/@NAME@
chmod 700 ./usr/java/@NAME@

cp -v %{SOURCE0} ./usr/java/@NAME/@MODULE_NAME@.jar

cat -> ./usr/java/@NAME@/@MODULE_NAME@.deps <<EOF
@DEPS_FILE_CONTENTS@
EOF

@BUILD_EXEC_FILES@

%clean
rm -rf $RPM_BUILD_ROOT

%files
%defattr(-,root,root,-)
/usr/java/@NAME@
%config /usr/java/@NAME@/@MODULE_NAME@.deps

@EXEC_PATHS@
